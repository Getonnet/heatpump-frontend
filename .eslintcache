  }\r\n\r\n  return (\r\n    <div className='App' style={{ backgroundImage: `url('${bodyBgImage}')` }}>\r\n      <Router>\r\n        <Header items={cart} />\r\n\r\n        <Switch>\r\n          <Route path=\"/cart\">\r\n            <CartPage items={item} total={amounts} />\r\n          </Route>\r\n          <Route path=\"/brands\">\r\n            <BrandsSelect />\r\n          </Route>\r\n          <Route path=\"/\">\r\n            <Homepage myChange={handleChange} />\r\n          </Route>\r\n        </Switch>\r\n\r\n        <AssistantPerson />\r\n      </Router>\r\n    </div>\r\n  )\r\n}\r\n\r\nexport default App\r\n","C:\\wamp64\\www\\heatpump-frontend\\src\\reportWebVitals.js",[],"C:\\wamp64\\www\\heatpump-frontend\\src\\components\\header.jsx",[],"C:\\wamp64\\www\\heatpump-frontend\\src\\components\\assistant-person.jsx",[],"C:\\wamp64\\www\\heatpump-frontend\\src\\pages\\home.jsx",["99"],"import React, { useState, useEffect } from 'react';\r\n// import OptionsAndInfoBox from '../components/optionsAndInfoBox'\r\nimport ProductCard from '../components/product-card'\r\nimport '../styles/components/_suggested-products.scss'\r\nimport configure from \"../config.js\"\r\nimport axios from 'axios';\r\n\r\nexport default function Homepage({myChange}) {\r\n\r\n  const [products, setProducts] = useState([])\r\n\r\n  useEffect(async () => {\r\n\r\n    let response = await axios.get(configure.API_URL+'product-recommend');\r\n\r\n    await setProducts(response.data);\r\n\r\n\r\n  }, []);\r\n\r\n  return (\r\n    <div className='page-home'>\r\n      <div className='container'>\r\n        {/*<OptionsAndInfoBox />*/}\r\n        <div className='suggested-products'>\r\n          {\r\n            products.map( row => (\r\n              <ProductCard key={row.id} products={row.id} name={row.name} img={row.photo} price={row.price} descriptions={row.descriptions} category={row.category} changes={myChange} />\r\n            ))\r\n          }\r\n\r\n        </div>\r\n      </div>\r\n    