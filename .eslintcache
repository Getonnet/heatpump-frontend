rt'>\r\n          <table className='cart-table'>\r\n            <thead>\r\n              <tr>\r\n                <th>Product</th>\r\n                <th>Brief Info</th>\r\n                <th>Quantity</th>\r\n                <th>Price</th>\r\n              </tr>\r\n            </thead>\r\n\r\n            <tbody>\r\n              {\r\n                items.map(row => (\r\n                  <CartLineItem key={row.id} product={row.id} name={row.names} price={row.prices} photo={row.photo} quantity={row.qty} />\r\n                ))\r\n              }\r\n              {/*cart total*/}\r\n              <tr>\r\n                <td />\r\n                <td>\r\n                  <span className='discount'>Total</span>\r\n                </td>\r\n                <td />\r\n                <td>{total} kr</td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n        </div>\r\n\r\n        <div className='cart-cta'>\r\n          <button className='btn' onClick={openModal}>\r\n            Send your request\r\n          </button>\r\n        </div>\r\n      </div>\r\n\r\n      <Modal\r\n        isOpen={modalIsOpen}\r\n        onAfterOpen={afterOpenModal}\r\n        onRequestClose={closeModal}\r\n        style={customModalStyles}\r\n        ariaHideApp={false}\r\n        contentLabel='Example Modal'>\r\n        <div className='modal-header'>\r\n          <h2>Contact Form</h2>\r\n          <span onClick={closeModal}>\r\n            <ModalCloseIcon />\r\n          </span>\r\n        </div>\r\n\r\n        <form className='collect-data-form'>\r\n          <div className='left'>\r\n            <div className='form-field'>\r\n              <label htmlFor='name'>Your Name</label>\r\n              <div className='input-icon'>\r\n                <div className='icon'>\r\n                  <InputNameIcon />\r\n                </div>\r\n                <div className='input'>\r\n                  <input type='text' id='name' name=\"name\" onChange={handleChange} value={data.name || ''} placeholder='name' />\r\n                </div>\r\n              </div>\r\n            </div>\r\n\r\n            <div className='form-field'>\r\n              <label htmlFor='email'>Your Email</label>\r\n              <div className='input-icon'>\r\n                <div className='icon'>\r\n                  <InputEmailIcon />\r\n                </div>\r\n                <div className='input'>\r\n                  <input type='text' id='email' name=\"email\" onChange={handleChange} value={data.email || ''} placeholder='email' />\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div className='form-field'>\r\n              <label htmlFor='phone'>Phone No.</label>\r\n              <div className='input-icon'>\r\n                <div className='icon'>\r\n                  <InputPhoneIcon />\r\n                </div>\r\n                <div className='input'>\r\n                  <input type='text' id='phone' name=\"phone\" onChange={handleChange} value={data.phone || ''} placeholder='phone' />\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <div className='right'>\r\n            <div className='form-field'>\r\n              <label htmlFor='message'>Message</label>\r\n              <textarea name='message' id='message' name=\"message\" onChange={handleChange} value={data.message || ''} rows='100%' />\r\n            </div>\r\n          </div>\r\n\r\n          <div className='bottom'>\r\n            <button type='submit' className='btn'>\r\n              Send Request\r\n            </button>\r\n          </div>\r\n        </form>\r\n      </Modal>\r\n    </div>\r\n  )\r\n}\r\n","D:\\soly\\heatpump-frontend\\src\\config.js",[],"D:\\soly\\heatpump-frontend\\src\\images\\input-email-icon.js",[],"D:\\soly\\heatpump-frontend\\src\\images\\input-phone-icon.js",[],"D:\\soly\\heatpump-frontend\\src\\images\\input-name-icon.js",[],"D:\\soly\\heatpump-frontend\\src\\im