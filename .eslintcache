rrentItem.findIndex(x => x.id == productID);\n\n       if(i > -1){ // Update product quantity if exist\n        currentItem[i].qty = quantity;\n       }else{\n        currentItem.push({id:productID, qty:quantity, prices: price, names: name, photo: img});\n       }\n       setItem(currentItem);\n    }else{\n      let curItems = currentItem.filter((x, index, arr) => x.id !== productID);\n      setItem(curItems);\n    }\n    setCart(item.length);\n\n    getTotal();\n  }\n\n  const getTotal = () => {\n\n   let amount = item.map(x => (\n     x.qty * x.prices\n   ));\n\n   let total = amount.reduce((a,b) => {\n     return a+b;\n   }, 0);\n\n    setAmounts(total);\n  }\n\n  useEffect( () => {\n    window.kindlyOptions = {\n      onMessage: (newMessage, chatLog) => {\n        setChats( [ ...chatLog ] );\n        // setMessages( [ ...messages,newMessage ] );\n      }\n    }\n});\n\n  return (\n    <div className='App' style={{ backgroundImage: `url('${bodyBgImage}')` }}>\n      <Router>\n        <Header items={cart} />\n        \n        <Switch>\n          <Route path=\"/cart\">\n            <CartPage items={item} total={amounts} />\n          </Route>\n          <Route path=\"/brands\">\n            <BrandsSelect />\n          </Route>\n          <Route path=\"/\">\n            <Homepage myChange={handleChange} />\n          </Route>\n        </Switch>\n        <AssistantPerson />\n      </Router>\n      {/* <KindlyOwnChat/> */}\n    </div>\n  )\n}\n\nexport default App\n","/Users/rokibul/sites/getonnet/heatpump-frontend/src/pages/brandSelect.jsx",[],"/Users/rokibul/sites/getonnet/heatpump-frontend/src/components/assistant-person.jsx",[],"/Users/rokibul/sites/getonnet/heatpump-frontend/src/components/header.jsx",[],"/Users/rokibul/sites/getonnet/heatpump-frontend/src/pages/cartpage.jsx",[],"/Users/rokibul/sites/getonnet/heatpump-frontend/src/pages/home.jsx",["106"],"import React, { useState, useEffect } from 'react';\n// import OptionsAndInfoBox from '../components/options